{
  "name": "martyx-industries-fe",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.19.0 <21"
  },
  "scripts": {
    "dev": "vite --config vite.config.optimized.ts",
    "build": "tsc -b && vite build --config vite.config.optimized.ts",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist",
    "build:report": "npm run build -- --mode analyze",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist node_modules/.vite node_modules/.cache",
    "optimize": "npm run clean && npm install && npm run build"
  },
  "dependencies": {
    "@google/model-viewer": "^4.1.0",
    "@paypal/paypal-js": "^8.2.0",
    "@paypal/react-paypal-js": "^8.8.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.7.1",
    "three": "^0.172.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "rimraf": "^6.0.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^4.5.0",
    "vite-bundle-analyzer": "^0.12.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "packageManager": "npm@10.0.0"
}
